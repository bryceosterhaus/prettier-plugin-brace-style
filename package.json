{
  "name": "prettier-plugin-brace-style",
  "version": "0.1.1",
  "description": "A Prettier plugin that can apply ESLint's brace-style rules to JavaScript and TypeScript files.",
  "repository": "https://github.com/ony3000/prettier-plugin-brace-style",
  "keywords": [
    "prettier",
    "plugin",
    "brace",
    "style"
  ],
  "author": "Hyeonjong <nianelo4@gmail.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/prettier-plugin-brace-style.umd.cjs",
  "module": "./dist/prettier-plugin-brace-style.js",
  "exports": {
    ".": {
      "import": "./dist/prettier-plugin-brace-style.js",
      "require": "./dist/prettier-plugin-brace-style.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "lint": "eslint src",
    "test": "jest --ci --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "29.5.2",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-node": "0.3.7",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "jest": "29.5.0",
    "prettier": "~2.8.4",
    "ts-jest": "29.1.0",
    "typescript": "4.9.5",
    "vite": "4.3.9"
  },
  "peerDependencies": {
    "prettier": "~2.8.4"
  },
  "optionalDependencies": {
    "prettier-plugin-merge": "~0.1.0"
  }
}
